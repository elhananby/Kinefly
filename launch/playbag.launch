<launch>
	<!-- ========================================================== -->
	<!-- Launch everything needed for Kinefly.                      -->
	<!-- ========================================================== -->


    <include file="$(find Kinefly)/launch/params_kinefly.launch" />
    <include file="$(find Kinefly)/launch/camera_playbag.launch" /> 

    <node name="kinefly"                 pkg="Kinefly"  type="kinefly.py" required="true"      />
    <node name="kinefly2phidgetsanalog"  pkg="Kinefly"  type="kinefly2phidgetsanalog.py"  />
    <node name="kinefly2ledpanels"       pkg="Kinefly"  type="kinefly2ledpanels.py"  />
    
</launch>
