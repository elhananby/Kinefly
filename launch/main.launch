<launch>
	<!-- ========================================================== -->
	<!-- Launch everything needed for strokelitude.                 -->
	<!-- ========================================================== -->


    <include file="$(find StrokelitudeROS)/launch/params_strokelitude.launch" ns="strokelitude" />
    <include file="$(find StrokelitudeROS)/launch/params_camera1394.launch"   ns="camera" />


    <include file="$(find StrokelitudeROS)/launch/camera_1394.launch" />


    <node name="strokelitude"            pkg="StrokelitudeROS"  type="strokelitude.py"           required="true"      />
    <node name="strokelitude2ledpanels"  pkg="StrokelitudeROS"  type="strokelitude2ledpanels.py"  />
    <node name="ledpanels"               pkg="ledpanels"        type="ledpanels.py"  />
    
</launch>
