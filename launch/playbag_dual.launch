<launch>
	<!-- ========================================================== -->
	<!-- Launch everything needed for Kinefly.                      -->
	<!-- ========================================================== -->


    <include file="$(find Kinefly)/launch/params_kinefly_1.launch" />
    <include file="$(find Kinefly)/launch/params_kinefly_2.launch" />

    <node name="image_proc" pkg="image_proc" type="image_proc" ns="camera1" />
    <node name="image_proc" pkg="image_proc" type="image_proc" ns="camera2" />
    
    <node name="playbag"    pkg="rosbag"     type="play" args="--clock --delay=1 --rate=1.0 --loop /home/ssafarik/bags/kinefly/dual_1.bag" required="true" /> 
    <node name="kinefly1"   pkg="Kinefly"    type="kinefly.py"  required="true"      />
    <node name="kinefly2"   pkg="Kinefly"    type="kinefly.py"  required="true"      />
    
</launch>
