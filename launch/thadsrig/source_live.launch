<launch>
	<!-- ========================================================== -->
	<!-- Source images for Kinefly.                                 -->
	<!-- ========================================================== -->
    <param name="use_sim_time"                 type="bool" value="false"/>


    <include file="$(find Kinefly)/launch/$(env RIG)/camera_ethernet.launch" />
    <include file="$(find Kinefly)/launch/$(env RIG)/params_camera.launch"                     ns="camera" />


    <!-- Get and publish the voltage signals from hardware. --> 
    <include file="$(find Kinefly)/launch/$(env RIG)/params_stimulus.launch" />
    <node name="interfacekit"             pkg="phidgets"  type="phidgets_interfacekit.py"   ns="stimulus"     required="true" />
    <node name="publish_voltages"         pkg="Kinefly"   type="publish_voltages.py"        ns="stimulus"     required="true" />


</launch>
