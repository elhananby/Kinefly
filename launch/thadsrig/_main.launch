<launch>
	<!-- ========================================================== -->
	<!-- Launch everything needed for Kinefly, except the           -->
    <!-- source image streams.                                      -->
	<!-- ========================================================== -->

    <include file="$(find Kinefly)/launch/$(env RIG)/params_kinefly.launch"                    ns="kinefly" />


    <node name="flystate2phidgetsanalog"  pkg="Kinefly"   type="flystate2phidgetsanalog.py" ns="kinefly" />
    <node name="kinefly"                 pkg="Kinefly"   type="kinefly.py"                                  required="true" />

    <rosparam param="stimulus/publish_voltages/channels_ai" >[]</rosparam>
    <rosparam param="stimulus/publish_voltages/channels_di" >[0]</rosparam>
    <node name="interfacekit"            pkg="phidgets"  type="phidgets_interfacekit.py"  ns="stimulus"     required="true" />
    <node name="publish_voltages"        pkg="Kinefly"   type="publish_voltages.py"       ns="stimulus"     required="true" />
    


</launch>
